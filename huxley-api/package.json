  "license": "MIT",
  "devDependencies": {
    "amen": "^1.0.0-alpha-03",
  },
  "dependencies": {
    "pbx": "1.0.0-alpha-14",
    "fairmont": "^0.8.2",
    "pirate": "^1.0.0-alpha-05"
    "key-forge": "^1.0.0-beta-01",
    "panda-cluster": "git://github.com/pandastrike/panda-cluster.git",
    "panda-hook": "git://github.com/pandastrike/panda-hook",
    "when": "^3.6.3"
  },
  "engines": {
    "node": ">=0.11 < 0.12"
  }
}
