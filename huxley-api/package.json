    "panda-cluster": "git://github.com/pandastrike/panda-cluster.git",
    "panda-config": "^1.0.0-alpha-02",
    "panda-hook": "git://github.com/pandastrike/panda-hook",
    "pbx": "1.0.0-alpha-14",
